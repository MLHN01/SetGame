<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="790.0" maxWidth="1540.0" minHeight="790.0" minWidth="1540.0" stylesheets="@../css/application2.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.setgame.setgame.ui.controller.ScoreBoardController">
    <ImageView fitHeight="790.0" fitWidth="1540.0">
        <image>
            <Image url="@../images/background.png" />
        </image>
    </ImageView>
    <Label id="textTitle" layoutX="468.0" text="SCORE BOARD">
      <effect>
         <Glow level="0.7" />
      </effect></Label>
    <Button fx:id="backButton" layoutX="665.0" layoutY="665.0" mnemonicParsing="false" styleClass="black-button" text="BACK">
      <effect>
         <Glow level="0.75" />
      </effect></Button>
    <TableView fx:id="table" layoutX="364.0" layoutY="135.0" prefHeight="540.0" prefWidth="760.0">
        <columns>
            <TableColumn fx:id="Rank" prefWidth="190.0" text="Rank" />
            <TableColumn fx:id="Score" minWidth="190.0" prefWidth="180.0" text="Score" />
            <TableColumn fx:id="Date" prefWidth="190.0" text="Date" />
            <TableColumn fx:id="Name" prefWidth="190.0" text="Name" />
        </columns>
    </TableView>
</AnchorPane>
